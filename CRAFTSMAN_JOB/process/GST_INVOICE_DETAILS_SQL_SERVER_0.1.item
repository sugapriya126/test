    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;SELECT  invoice_no,&#13;&#10;         excise_invoice_no,&#13;&#10;         excise_invoice_date,&#13;&#10;         trx_date,&#13;&#10;         tariff, &#13;&#10;         tax_igst,&#13;&#10;         tax_cgst_sgst,&#13;&#10;         division,&#13;&#10;         customer_name,&#13;&#10;         division_id,&#13;&#10;         party_site_name,&#13;&#10;         order_header_id,&#13;&#10;         order_line_id,&#13;&#10;         delivery_detail_id,&#13;&#10;         (tot_amount-discount+igst+cgst+sgst+tcs+freight+packing)tot_amount,&#13;&#10;         ass_value,&#13;&#10;         (bill_amt-discount+igst+cgst+sgst+tcs+freight+packing)bill_amt,&#13;&#10;         quantity,&#13;&#10;         (case  when tax_igst is not null &#13;&#10;             then tax_igst&#13;&#10;             when tax_cgst_sgst is not null then&#13;&#10;              tax_cgst_sgst &#13;&#10;   end)tax,  &#13;&#10;         freight,&#13;&#10;         packing,&#13;&#10;         discount,&#13;&#10;         igst,&#13;&#10;         cgst,&#13;&#10;         sgst,&#13;&#10;         tcs,&#13;&#10;         order_type,&#13;&#10;         INV_TYPE,&#13;&#10;         attribute14,&#13;&#10;         org_id,&#13;&#10;         period_name,&#13;&#10;         TYPE,&#13;&#10;         Order_Type_P&#13;&#10;FROM (SELECT DISTINCT r.invoice_no,&#13;&#10;         r.excise_invoice_no,&#13;&#10;         r.excise_invoice_date,&#13;&#10;         r.trx_date,&#13;&#10;         r.tariff,&#13;&#10;        (CASE&#13;&#10;              WHEN r.inv_type IN ('SO', 'ISO')&#13;&#10;                   AND r.DELIVERY_DETAIL_ID IS NOT NULL&#13;&#10;              THEN&#13;&#10;                 (SELECT   DISTINCT jtc.tax_category_name&#13;&#10;                    FROM   jai_tax_categories jtc,&#13;&#10;                           jai_tax_category_lines jtcl,&#13;&#10;                           jai_tax_lines jtla&#13;&#10;                   WHERE       jtla.trx_line_id = r.order_line_id&#13